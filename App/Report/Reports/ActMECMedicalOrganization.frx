      <TableDataSource Name="TableLocalUser" Alias="localUser" DataType="System.Int32" PropName="Table" Enabled="true" TableName="localUser" SelectCommand="SELECT L.UserID, L.LastName, L.FirstName, L.Patronymic, L.Position, L.Phone, L.ConfNumber&#13;&#10;FROM &quot;localUser&quot; L&#13;&#10;WHERE L.UserID=@UserID&#13;&#10;">
