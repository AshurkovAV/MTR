        <Column Name="kolvo" DataType="System.Int32"/>
        <Column Name="price" DataType="System.Decimal"/>
        <Column Name="refuse" DataType="System.Int32"/>
        <Column Name="refuseprice" DataType="System.Decimal"/>
        <Column Name="accept" DataType="System.Int32"/>
        <Column Name="AcceptPrice" DataType="System.Decimal"/>
        <Column Name="EventType" DataType="System.Int32"/>
        <Column Name="EventTypeName" DataType="System.String"/>
        <Column Name="RegionalAttribute" DataType="System.Int32"/>
        <Column Name="RegionalAttributeName" DataType="System.String"/>
        <Column Name="AssistanceConditions" DataType="System.Decimal"/>
        <Column Name="AssistanceConditionsName" DataType="System.String"/>
        <CommandParameter Name="MedicalAccountIdParam" DataType="8" Expression="[MedicalAccountId]" DefaultValue="1038"/>
