      <TableDataSource Name="Table3" Alias="EventByProfile" DataType="System.Int32" Enabled="true" SelectCommand="SELECT &#13;&#10;u.PRNAME, EventType ,EventTypeName , RegionalAttribute, RegionalAttributeName, AssistanceConditions, AssistanceConditionsName,&#13;&#10;COUNT(*) kolvo,&#13;&#10;sum(isnull(MoPrice,0)) price,&#13;&#10;SUM(case when refuseprice = 0 then 0 else 1 end) as refuse,&#13;&#10;SUM(refuseprice) as refuseprice,&#13;&#10;SUM(case when refuseprice = 0 then 1 else 0 end) as accept,&#13;&#10;SUM(isnull(AcceptPrice,Price)) as AcceptPrice&#13;&#10;from &#13;&#10;(select e.*, V002.PRNAME, &#13;&#10;(SELECT MIN(Price) FROM (VALUES&#13;&#10;(&#13;&#10;isnull((SELECT MedicalEventId =&#13;&#10;	(&#13;&#10;			SELECT SUM(Amount)&#13;&#10;					FROM &quot;FactMEC&quot;&#13;&#10;					WHERE MedicalEventId =  E.EventId AND (IsLock &lt;&gt; 1 OR IsLock IS NULL)AND Source IN (1,3)&#13;&#10;	)),0)&#13;&#10;&#13;&#10;) ,(E.MoPrice)) AS AllPrices(Price)) refuseprice&#13;&#10;&#13;&#10;FROM &quot;EventShortView&quot; E &#13;&#10;  inner join v002 on v002.IDPR = e.Profile and e.MedicalAccountId = @MedicalAccountIdParam) u&#13;&#10;group by PRNAME, EventType ,EventTypeName , RegionalAttribute, RegionalAttributeName, AssistanceConditions, AssistanceConditionsName&#13;&#10;">
