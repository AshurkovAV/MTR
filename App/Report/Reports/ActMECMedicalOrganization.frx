      <TableDataSource Name="EventShortView" DataType="System.Int32" PropName="Table4" Enabled="true" TableName="EventShortView" SelectCommand="SELECT DISTINCT E.MedicalExternalId,E.MedicalAccountId, E.EventId, &#13;&#10;(case when E.InsuranceDocNumber IS null OR ltrim(E.InsuranceDocNumber) = '' then E.INP else E.InsuranceDocSeries+E.InsuranceDocNumber end) as INP, &#13;&#10;E.AssistanceConditions, E.AssistanceConditionsName, E.Diagnosis, E.MoPrice, E.AcceptPrice, E.EventBegin, E.EventEnd, &#13;&#10;E.MoPaymentStatus, E.EventComments, E.EventType, E.RegionalAttribute,E.EventTypeName, E.RegionalAttributeName,(SELECT MedicalEventId =&#13;&#10;(&#13;&#10;    SELECT CONVERT(nvarchar(30),F1.Osn) + ','	&#13;&#10;        FROM &quot;FactMEC&quot;&#13;&#10;				INNER JOIN &quot;F014&quot; F1 ON ReasonId = F1.Id &#13;&#10;				WHERE MedicalEventId =  E.EventId AND (IsLock &lt;&gt; 1 OR IsLock IS NULL) AND Source IN (1,3)&#13;&#10;        ORDER BY ReasonId&#13;&#10;        FOR XML PATH(''), TYPE&#13;&#10;).value('.[1]', 'NVARCHAR(MAX)')) as MecRefusals,&#13;&#10;(SELECT MedicalEventId =&#13;&#10;(&#13;&#10;    SELECT CONVERT(nvarchar(1000),Comments) + ' '	&#13;&#10;        FROM &quot;FactMEC&quot;&#13;&#10;				WHERE MedicalEventId =  E.EventId AND (IsLock &lt;&gt; 1 OR IsLock IS NULL)AND Source IN (1,3)&#13;&#10;        ORDER BY ReasonId&#13;&#10;        FOR XML PATH(''), TYPE&#13;&#10;).value('.[1]', 'NVARCHAR(MAX)')) as MecRefusalsComment,&#13;&#10;(SELECT MIN(Price) FROM (VALUES ((SELECT MedicalEventId =&#13;&#10;	(&#13;&#10;			SELECT SUM(Amount)&#13;&#10;					FROM &quot;FactMEC&quot;&#13;&#10;					WHERE MedicalEventId =  E.EventId AND (IsLock &lt;&gt; 1 OR IsLock IS NULL)AND Source IN (1,3)&#13;&#10;	))),(E.MoPrice)) AS AllPrices(Price))&#13;&#10; as MecRefusalsPrice&#13;&#10;FROM &quot;EventShortView&quot; E INNER JOIN &quot;FactMEC&quot; F ON E.EventId = F.MedicalEventId &#13;&#10;&#13;&#10;WHERE E.MedicalAccountId=@MedicalAccountParam AND (SELECT Count(MecId) FROM &quot;FactMec&quot; Where MedicalEventId=E.EventId AND  (IsLock &lt;&gt; 1 OR IsLock IS NULL) ) &gt; 0 AND Source IN (1,3) AND&#13;&#10;(SELECT Count(ExternalRefuseId) FROM &quot;FactExternalRefuse&quot; Where MedicalEventId=E.EventId ) = 0&#13;&#10;&#13;&#10;&#13;&#10;">
