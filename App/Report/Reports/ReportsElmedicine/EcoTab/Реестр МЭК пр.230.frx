      <TableDataSource Name="Table9" Alias="Table1" DataType="System.Int32" Enabled="true" SelectCommand="declare @sumv numeric(15,2), @kol_schetov int, @usl_ok int&#13;&#10;declare @osumv numeric(15,2), @sumv1 numeric(15,2), @sumv2 numeric(15,2), @sumv3 numeric(15,2)&#13;&#10;declare @kol_schetov1 int, @kol_schetov2 int, @kol_schetov3 int&#13;&#10;--declare @t table &#13;&#10;--(&#13;&#10;--all_sum decimal(15,2),&#13;&#10;--all_kol int,&#13;&#10;--sumv1 decimal(15,2),&#13;&#10;--sumv2 decimal(15,2),&#13;&#10;--sumv3 decimal(15,2),&#13;&#10;--kol1 int,&#13;&#10;--kol2 int,&#13;&#10;--kol3 int&#13;&#10;--)&#13;&#10;&#13;&#10;DECLARE vend_cursor CURSOR FOR &#13;&#10;select sum(sumv) as sumv, sum(kolvo) as kol_pol, usl_ok&#13;&#10;from(&#13;&#10;SELECT sum(sl.sumv) as sumv, count(*) as kolvo, usl_ok, sl.SCHET_ID&#13;&#10;FROM SLUCH sl&#13;&#10;join PACIENT pa on sl.PID = pa.ID and (pa.SMO is null or pa.SMO not like '46%')&#13;&#10;join SCHET sc on sl.SCHET_ID = sc.ID &#13;&#10;--join sank sa on sa.slid = sl.id and sa.S_TIP = 1&#13;&#10;where sl.SCHET_ID =@p&#13;&#10;and usl_ok in (1,2,3)&#13;&#10;group by sl.usl_ok, sl.SCHET_ID)t&#13;&#10;group by usl_ok&#13;&#10;order by usl_ok&#13;&#10;&#13;&#10;OPEN vend_cursor&#13;&#10;FETCH NEXT FROM vend_cursor&#13;&#10;into @sumv, @kol_schetov, @usl_ok &#13;&#10;&#13;&#10;WHILE @@FETCH_STATUS = 0&#13;&#10;begin&#13;&#10;	if @usl_ok = 1&#13;&#10;	begin&#13;&#10;		set @sumv1 = @sumv;&#13;&#10;		set @kol_schetov1 = @kol_schetov		&#13;&#10;	end;&#13;&#10;	if @usl_ok = 2&#13;&#10;	begin&#13;&#10;		set @sumv2 = @sumv;&#13;&#10;		set @kol_schetov2 = @kol_schetov		&#13;&#10;	end;&#13;&#10;	if @usl_ok = 3&#13;&#10;	begin&#13;&#10;		set @sumv3 = @sumv;&#13;&#10;		set @kol_schetov3 = @kol_schetov		&#13;&#10;	end;&#13;&#10;	FETCH NEXT FROM vend_cursor&#13;&#10;	into @sumv, @kol_schetov, @usl_ok	&#13;&#10;end;&#13;&#10;CLOSE vend_cursor;&#13;&#10;DEALLOCATE vend_cursor;&#13;&#10;&#13;&#10;--select isnull(@sumv1, 0)+isnull(@sumv2, 0)+isnull(@sumv3, 0) as osum, &#13;&#10;--isnull(@sumv1, 0) sumv1, isnull(@kol_schetov1, 0) kol_schetov1, &#13;&#10;--isnull(@sumv2, 0) sumv2, isnull(@kol_schetov2, 0) kol_schetov2, &#13;&#10;--isnull(@sumv3, 0) sumv3, isnull(@kol_schetov3, 0) kol_schetov3 &#13;&#10;&#13;&#10;--insert into @t(all_sum, sumv1, kol1, sumv2, kol2, sumv3, kol3)&#13;&#10;--VALUES (isnull(@sumv1, 0)+isnull(@sumv2, 0)+isnull(@sumv3, 0), &#13;&#10;--isnull(@sumv1, 0), isnull(@kol_schetov1, 0), &#13;&#10;--isnull(@sumv2, 0), isnull(@kol_schetov2, 0), &#13;&#10;--isnull(@sumv3, 0), isnull(@kol_schetov3, 0))&#13;&#10;&#13;&#10;select (isnull(@sumv1, 0)+isnull(@sumv2, 0)+isnull(@sumv3, 0))-(sumv1 + sumv2 + sumv3)all_sum,&#13;&#10;(isnull(@kol_schetov1, 0)+isnull(@kol_schetov2, 0)+isnull(@kol_schetov3, 0))-(kol1 + kol2 + kol3)all_kol,&#13;&#10;isnull(@sumv1, 0)-sumv1 as sumv1,&#13;&#10;isnull(@sumv2, 0)-sumv2 as sumv2,&#13;&#10;isnull(@sumv3, 0)-sumv3 as sumv3,&#13;&#10;isnull(@kol_schetov1, 0)-kol1 as kol1,&#13;&#10;isnull(@kol_schetov2, 0)-kol2 as kol2,&#13;&#10;isnull(@kol_schetov3, 0)-kol3 as kol3   &#13;&#10;from(&#13;&#10;SELECT sum(case when sl.usl_ok = 1 then sl.sank_it else 0 end) as sumv1,&#13;&#10;sum(case when usl_ok = 1 then 1 else 0 end) as kol1,&#13;&#10;sum(case when usl_ok = 2 then sl.sank_it else 0 end) as sumv2,&#13;&#10;sum(case when usl_ok = 2 then 1 else 0 end) as kol2,&#13;&#10;sum(case when usl_ok = 3 then sl.sank_it else 0 end) as sumv3,&#13;&#10;sum(case when usl_ok = 3 then 1 else 0 end) as kol3&#13;&#10;FROM SLUCH sl&#13;&#10;join SCHET sc on sl.SCHET_ID = sc.ID &#13;&#10;join PACIENT pa on sl.PID = pa.ID and (pa.SMO is null or pa.SMO not like '46%')&#13;&#10;join sank sa on sa.slid = sl.id and sa.S_TIP = 1&#13;&#10;where sl.SCHET_ID =@p&#13;&#10;and usl_ok in (1,2,3)&#13;&#10;and oplata = 2) t&#13;&#10;&#13;&#10;">
