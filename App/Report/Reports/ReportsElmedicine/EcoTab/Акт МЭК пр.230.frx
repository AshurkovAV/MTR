      <TableDataSource Name="Table6" DataType="System.Int32" Enabled="true" SelectCommand="Declare @t TABLE&#13;&#10;(&#13;&#10;USL Varchar(300),&#13;&#10;TypeSl varchar(300),&#13;&#10;os varchar(300),&#13;&#10;PROFIL varchar(300),&#13;&#10;count_vist int,&#13;&#10;sum_vist numeric (15,2),&#13;&#10;count_otkaz int,&#13;&#10;sum_otkaz numeric (15,2)&#13;&#10;)&#13;&#10;&#13;&#10;Declare @t1 TABLE&#13;&#10;(&#13;&#10;RN int,&#13;&#10;USL Varchar(300),&#13;&#10;TypeSl varchar(300),&#13;&#10;os varchar(300),&#13;&#10;PROFIL varchar(300),&#13;&#10;SUMV numeric (15,2),&#13;&#10;S_DATE datetime,&#13;&#10;SUMP numeric (15,2),&#13;&#10;SANK_IT numeric (15,2),&#13;&#10;UMPNAME Varchar(300),&#13;&#10;stName Varchar(300),&#13;&#10;osName Varchar(300),&#13;&#10;NameWithID Varchar(300)&#13;&#10;)&#13;&#10;INSERT INTO @t1&#13;&#10;SELECT ROW_NUMBER() OVER (PARTITION BY sl.ID&#13;&#10;                                       ORDER BY sl.ID) RN, v6.UMPNAME as USL, st.Name as TypeSl, ISNULL(os.Name, '-') as os,&#13;&#10;                                       v.NameWithID as PROFIL, ISNULL(sl.SUMV, 0), s.S_DATE, ISNULL(sl.SUMP, 0), ISNULL(sl.SANK_IT, 0),&#13;&#10;                                       v6.UMPNAME, st.Name as stName, os.Name as osName, v.NameWithID &#13;&#10;FROM SLUCH sl&#13;&#10;	join PACIENT pa on sl.PID = pa.ID&#13;&#10;	left join V002 v on sl.PROFIL = v.Id&#13;&#10;	left join V006 v6 on sl.USL_OK = v6.Id&#13;&#10;	left join TypeSluchDb st on sl.SLUCH_TYPE = st.Id&#13;&#10;	left join OsobSluchDb os on sl.OS_SLUCH_REGION = os.Id&#13;&#10;	left join SANK s on sl.ID = s.SLID&#13;&#10;Where sl.SCHET_ID = @p and (pa.SMO is null or pa.SMO not like '46%')&#13;&#10;&#13;&#10;Insert INTO @t&#13;&#10;SELECT&#13;&#10;	UMPNAME as USL, stName as TypeSl, ISNULL(osName, '-') as os,&#13;&#10;	NameWithID as PROFIL, &#13;&#10;	count(*) as count_vist,&#13;&#10;	sum(SUMV) as sum_vist,&#13;&#10;	sum((CASE WHEN ISNULL(SANK_IT, 0) &gt; 0 and S_DATE in ('__date__') THEN 1 END)) as count_otkaz,&#13;&#10;	sum((CASE WHEN ISNULL(SANK_IT, 0) &gt; 0 and S_DATE in ('__date__') THEN SANK_IT END)) as sum_otkaz&#13;&#10;FROM @t1 Where RN = 1&#13;&#10;Group By UMPNAME, stName, osName, NameWithID&#13;&#10;&#13;&#10;Select USL, TypeSl, os, PROFIL, count_vist, sum_vist, ISNULL(count_otkaz, 0) as count_otkaz, ISNULL(sum_otkaz, 0) as sum_otkaz, &#13;&#10;count_vist - ISNULL(count_otkaz, 0) as count_prin, sum_vist - ISNULL(sum_otkaz, 0) as sum_prin  FROM @t ">
